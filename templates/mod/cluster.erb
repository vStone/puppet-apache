### File managed with puppet ###
<% if scope.lookupvar('::servername') -%>
# Served by:        '<%= scope.lookupvar('::servername') %>'
<% end -%>
# Module:           '<%= @module_name %>'
# Template source:  'MODULES<%= template_source.gsub(Regexp.new("^#{Puppet.lookup(:current_environment)[:modulepath].gsub(':','|')}"),"") %>'

# mod_proxy_cluster
<% if @createbalancers and ! @createbalancers.empty? -%>
CreateBalancers <%= @createbalancers %>
<% end -%>
<% if @usealias and ! @usealias.empty? -%>
UseAlias <%= @usealias %>
<% end -%>
<% if @lbstatus_recal_time and ! @lbstatus_recal_time.empty?  -%>
LBstatusRecalTime  <%= @lbstatus_recal_time %>
<% end -%>
<% if @wait_for_remove and ! @wait_for_remove.empty? -%>
WaitForRemove  <%= @wait_for_remove %>
<% end -%>

# mod_manager
<% if @mem_manager_file and ! @mem_manager_file.empty? -%>
MemManagerFile <%= @mem_manager_file %>
<% end -%>
<% if @max_context and ! @max_context.empty? -%>
Maxcontext <%= @max_context %>
<% end -%>
<% if @max_node and ! @max_node.empty?  -%>
Maxnode <%= @max_node %>
<% end -%>
<% if @max_host and ! @max_host.empty? -%>
Maxhost <%= @max_host %>
<% end -%>
<% if @max_sessionid and ! @max_sessionid.empty? -%>
Maxsessionid <%= @max_sessionid %>
<% end -%>
<% if @persist_slots and ! @persist_slots.empty? -%>
PersistSlots <%= @persist_slots%>
<% end -%>

